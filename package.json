{
  "name": "teste",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.1"
  },
  "description": "Teste para processo seletivo",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "client-install": "cd client && npm install",
    "build-client": "cd client && npm build",
    "client": "npm start --prefix client",
    "dev": "concurrently \"ts-node ./lib/server.ts\" \"npm run client\"",
    "start": "nodemon ./dist/server.js",
    "server": "ts-node ./lib/server.ts",
    "prod": "npm run build && npm run start",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build && npm run client-install && npm run client-build"
  },
  "author": "Gabriel Guedes",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.6",
    "ts-node": "^7.0.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.1.6",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0"
  }
}
